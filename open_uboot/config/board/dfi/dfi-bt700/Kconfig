config UBOOT_CONFIG_SYS_BOARD
	default "dfi-bt700"

config UBOOT_CONFIG_SYS_VENDOR
	default "dfi"

config UBOOT_CONFIG_SYS_SOC
	default "baytrail"

config UBOOT_CONFIG_SYS_CONFIG_NAME
	default "dfi-bt700" if UBOOT_CONFIG_TARGET_Q7X_151_DFI_BT700
	default "theadorable-x86-dfi-bt700" if UBOOT_CONFIG_TARGET_THEADORABLE_X86_DFI_BT700

config UBOOT_CONFIG_SYS_TEXT_BASE
	default 0xfff00000

config UBOOT_CONFIG_BOARD_SPECIFIC_OPTIONS # dummy
	def_bool y
	select UBOOT_CONFIG_X86_RESET_VECTOR
	select UBOOT_CONFIG_INTEL_BAYTRAIL
	select UBOOT_CONFIG_BOARD_ROMSIZE_KB_8192
	select UBOOT_CONFIG_BOARD_EARLY_INIT_F
	select UBOOT_CONFIG_BOARD_LATE_INIT
	select UBOOT_CONFIG_SPI_FLASH_STMICRO
	imply SPI_FLASH_SPANSION
	imply SPI_FLASH_WINBOND
	select UBOOT_CONFIG_SERIAL_RX_BUFFER

config UBOOT_CONFIG_PCIE_ECAM_BASE
	default 0xe0000000
