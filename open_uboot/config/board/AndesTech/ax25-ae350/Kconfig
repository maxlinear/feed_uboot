	default "ax25-ae350"

config UBOOT_CONFIG_ENV_SIZE
	default 0x2000 if UBOOT_CONFIG_ENV_IS_IN_SPI_FLASH

config UBOOT_CONFIG_ENV_OFFSET
	default 0x140000 if UBOOT_CONFIG_ENV_IS_IN_SPI_FLASH

config UBOOT_CONFIG_SPL_TEXT_BASE
	default 0x800000

config UBOOT_CONFIG_SPL_OPENSBI_LOAD_ADDR
	default 0x01000000

config UBOOT_CONFIG_BOARD_SPECIFIC_OPTIONS # dummy
	def_bool y
	select UBOOT_CONFIG_RISCV_NDS
	select UBOOT_CONFIG_SUPPORT_SPL
	select UBOOT_CONFIG_BINMAN if SPL
	imply SMP
	imply SPL_RAM_SUPPORT
	imply SPL_RAM_DEVICE

