menu "mpc85xx CPU"
	depends on UBOOT_CONFIG_MPC85xx

config UBOOT_CONFIG_SYS_CPU
	default "mpc85xx"

config UBOOT_CONFIG_CMD_ERRATA
	bool "Enable the 'errata' command"
	depends on UBOOT_CONFIG_MPC85xx
	default y
	help
	  This enables the 'errata' command which displays a list of errata
	  work-arounds which are enabled for the current board.

choice
	prompt "Target select"
	optional

config UBOOT_CONFIG_TARGET_SOCRATES
	bool "Support socrates"
	select UBOOT_CONFIG_ARCH_MPC8544

config UBOOT_CONFIG_TARGET_P3041DS
	bool "Support P3041DS"
	select UBOOT_CONFIG_PHYS_64BIT
	select UBOOT_CONFIG_ARCH_P3041
	select UBOOT_CONFIG_BOARD_LATE_INIT if CHAIN_OF_TRUST
	imply CMD_SATA
	imply PANIC_HANG

