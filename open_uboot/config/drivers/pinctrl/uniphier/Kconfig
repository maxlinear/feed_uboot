if UBOOT_CONFIG_ARCH_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER
	bool

config UBOOT_CONFIG_PINCTRL_UNIPHIER_LD4
	bool "UniPhier LD4 SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_LD4
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER_PRO4
	bool "UniPhier Pro4 SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_PRO4
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER_SLD8
	bool "UniPhier sLD8 SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_SLD8
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER_PRO5
	bool "UniPhier Pro5 SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_PRO5
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER_PXS2
	bool "UniPhier PXs2 SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_PXS2
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

config UBOOT_CONFIG_PINCTRL_UNIPHIER_LD6B
	bool "UniPhier LD6b SoC pinctrl driver"
	depends on UBOOT_CONFIG_ARCH_UNIPHIER_LD6B
	default y
	select UBOOT_CONFIG_PINCTRL_UNIPHIER

